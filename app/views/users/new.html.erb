<h1>Sign Up</h1>


<%= form_for @user do |f| %>
	<%#= f.error_messages %>

	<!-- <div>
	  <%#= f.label :name %>
	  <%#= f.text_field :name %>
	</div> -->

	<div>
	  <%= f.label :email %>
	  <%= f.text_field :email %>
	</div>
<!-- 
	<div>
	  <%#= f.label :handle %>
	  <%#= f.text_field :handle %>
	</div> -->

	<div>
		<%= f.label :password %>
		<%= f.password_field :password %>
	</div>

	<div>
		<%= f.label :password_confirmation %>
		<%= f.password_field :password_confirmation %>
	</div>

	<%= f.submit %>
<% end %>
<%#= render 'form' %>
<%# end %>
